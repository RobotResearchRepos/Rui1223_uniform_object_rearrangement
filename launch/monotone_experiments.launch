<launch>
    <!-- launch specification file to get parameters for the task -->
    <include file="$(find uniform_object_rearrangement)/launch/task_specification.launch"></include>

    <!-- monotone experiments setting up -->
    <!-- arg (#experiment per object) -->
    <node pkg="uniform_object_rearrangement" type="MonotoneExperiments.py" name="monotone_experiments"
        args="$(arg monotone_experiments)" output="screen" required="True" />

    <!-- the following node expects argument RearrangementTaskPlanner <num_objects(int)> -->
    <!-- <node pkg="uniform_object_rearrangement" type="RearrangementTaskPlanner.py" name="rearrangement_task_planner"
        args="$(arg rearrangement_task_planner)" output="screen" required="False" /> -->

    <!-- the following node expects argument MRSplanner <num_objects(int)> <instance_number(int)> <GenerateOrLoad(char)> -->
    <!-- <node pkg="uniform_object_rearrangement" type="MRSplanner.py" name="mrs_task_planner"
        args="$(arg mrs_task_planner)" output="screen" required="False" /> -->

    <!-- the following node expects argument DFSDPplanner <num_objects(int)> <num_objects(int)> <instance_number(int)> <GenerateOrLoad(char)> -->
    <!-- <node pkg="uniform_object_rearrangement" type="DFSDPplanner.py" name="dfsdp_task_planner"
        args="$(arg dfsdp_task_planner)" output="screen" required="False" /> -->

    <node pkg="uniform_object_rearrangement" type="PybulletExecutionScene.py" name="pybullet_execution_scene"
		    output="screen" required="True" />

    <node pkg="uniform_object_rearrangement" type="PybulletPlanScene.py" name="pybullet_plan_scene"
		    output="screen" required="True" />

    <node pkg="uniform_object_rearrangement" type="PoseEstimator.py" name="fake_pose_estimator"
        output="screen" required="False" />

    <node pkg="uniform_object_rearrangement" type="main_planner_node" name="main_planner" output="screen" />
   
</launch>